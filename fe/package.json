{
  "name": "@icod2/fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "VITE_TEST=elomordo vite --host",
    "build": "tsc --build tsconfig.app.json && vite build",
    "build:analyze": "tsc --build tsconfig.app.json && vite build --mode analyze",
    "build:size": "yarn build && ls -lah dist/assets/",
    "analyze:css": "yarn build && echo 'CSS Bundle Size:' && du -h dist/assets/*.css",
    "analyze:js": "yarn build && echo 'JS Bundle Size:' && du -h dist/assets/*.js",
    "analyze:all": "yarn build && echo '=== Bundle Analysis ===' && du -h dist/assets/* && echo '=== Total Dist Size ===' && du -sh dist/",
    "analyze:components": "node analyze-components.js",
    "build-force": "tsc --build --force tsconfig.app.json && vite build",
    "lint": "yarn run -T biome check",
    "lint:fix": "yarn run -T biome check --write",
    "type-check": "tsc --noEmit --project tsconfig.app.json",
    "preview": "vite preview",
    "test": "vitest run"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@icod2/contracts": "workspace:*",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/themes": "^3.2.1",
    "@redux-devtools/extension": "^3.3.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.0.8",
    "date-fns": "^4.1.0",
    "detect-browser": "^5.3.0",
    "icod-crypto-js": "workspace:*",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.3",
    "tailwind-merge": "^3.3.1",
    "ua-parser-js": "^2.0.4",
    "zod": "^3.25.76",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "cssnano": "^7.1.0",
    "jsdom": "^26.1.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "yarn@4.9.2"
}
